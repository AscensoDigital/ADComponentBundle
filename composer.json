{
    "name": "ascensodigital/component-bundle",
    "type": "symfony-bundle",
    "description": "Componentes reutilizables para Symfony 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Corvalan",
            "email": "corvalanclaudio7@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "~2.7",
        "symfony/form": "~2.7",
        "symfony/twig-bundle": "~2.7",
        "symfony/validator": "~2.7",
        "symfony/yaml": "~2.7",
        "doctrine/orm": "~2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/phpunit-bridge": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "AscensoDigital\\ComponentBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ADComponentBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.2.2"
}
